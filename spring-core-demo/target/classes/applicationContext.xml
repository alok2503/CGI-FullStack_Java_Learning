<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean name="person" class="org.example.spring_core_demo.Person">
<property name ="personId" value="12"/>
<property name="first_name" value="Alok"/>
<property name="last_name" value="Mishra"/>
</bean>
<bean name="employee" class="org.example.spring_core_demo.Employee">
<property name="employeeId" value="468469"></property>
<property name="designation" value="Software Engineet"></property>
<property name="salary" value="60000"></property>
</bean>
</beans>
